          <field name="message_ids"/>
          <field name="activity_ids"/>
        </div>
          <notebook>
        <page string="Basic Information" >
          <group>
          <group name="left">
              <field name="attachments"  widget="many2many_binary"/>
              <field name="license_code" />
              <field name="license_date" />
              <field name="date_notarization" />
              <field name="license_notarization" />
              <field name="land_sketch"/>

              </group>
              <group>
               <field name="owner"/>
                <field name="price"/>

            </group>
              </group>
             </page>
             </notebook>
     </form>
      </field>
      </record>
      
      
      
      
