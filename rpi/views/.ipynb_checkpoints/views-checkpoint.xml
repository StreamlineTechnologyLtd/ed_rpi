 <!-- bulding form veiw --> 
      <record model="ir.ui.view" id="rpi_bulding_view_form">
      <field name="name">rpi.bulding.view.form</field>
      <field name="model">rpi.bulding</field>
      <field name="mode">primary</field>
      <field name="arch" type="xml">
        <form sample="1">
            <header>
                <button id="button_rented" name="action_rented_button" string="Rent" class="btn-primary" type="object"/>
                <button id="button_progress" name="action_progress_button" string="Project" class="btn-primary" type="object"/>
                <button id="button_loan" name="action_loan_button" string="Loan" class="btn-primary" type="object"/>
                <field name="bulding_status" widget="statusbar"/>
           </header>
           <sheet string="Bulding">
     <div class="oe_title">
                <h1>
                    <field name="name" required="1" placeholder="Name..."/>
                    
                </h1>
            </div>
        <group >
      <group name="left">
            <field name="code"  />
            <field name="region_id" />
            <field name="city_id" />
            <field name="country_id" />
          </group>

      <group name="right">
            <field name="purchase_date" />
            <field name="sale_date" />
          </group>
          </group>

  </sheet>
        <notebook>
        <page string="Basic Information" >
          <group>
          <group name="left">
            <field name="bulding_type" />
            <field name="bulding_stutes_id" />
            <field name="building_area" />
            <field name="land_area" />
            <field name="construction_year" />
            <field name="lift" />
            <field name="price" />
            <field name="bulding_sketch" />
            <field name="attachments"  widget="many2many_binary"/>
              
          </group>

          <group name="right">
            <field name="surface" />
            <field name="garage" />
            <field name="garden" />
          </group>

        </group>

      </page>
       <page string="Additional Information" >
          <group>
          <group>
            <field name="license_code" />
            <field name="license_date" />
            <field name="date_notarization" />
            <field name="license_notarization" />
        </group>       
        </group>           

      </page>
            
       <page string="Geoloation" >
          <group>       
        </group>           

      </page>
       <page string="note" >
          <group>  
        <field name="note" />
        </group>           
      </page>            
      </notebook>

  <div class="oe_chatter" name="oe_chatter">
    <field name="message_follower_ids"/>
    <field name="message_ids"/>
    <field name="activity_ids"/>
  </div>

        </form>
      </field>
    </record>
    
  
      
      
