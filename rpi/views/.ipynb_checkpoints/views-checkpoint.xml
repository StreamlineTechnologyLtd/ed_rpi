                <!-- Country form veiw --> 
    <record model="ir.ui.view" id="rpi_rpi_view_form">
      <field name="name">rpi.rpi.view.form</field>
      <field name="model">rpi.rpi</field>
      <field name="mode">primary</field>
      <field name="arch" type="xml">
      <form sample="1">
        <header/>
        <sheet string="Countries">
          <div class="oe_title">
                      <h1>
                          <field name="name" required="1" placeholder="Name..."/>
                      </h1>
                  </div>
          <group >
            <field name="density" placeholder="Population Density" />
             <field name="area" placeholder="Land Area m^2"/>
          </group>
        </sheet>
        <div class="oe_chatter" name="oe_chatter">
          <field name="message_follower_ids"/>
          <field name="message_ids"/>
          <field name="activity_ids"/>
        </div>
     </form>
      </field>
      </record>
      

      
             <!-- Country list veiw --> 
